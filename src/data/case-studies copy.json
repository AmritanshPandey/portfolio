            "thumbnail": "/assets/images/case-studies/thumbnail-1.jpg",
            "title": "Sole Hub",
            "timeline": "7 months",
            "challenge": "India's sneaker scene is vibrant yet scattered across brand sites, boutique raffles, and resale groups. Users struggle with discovering drops, verifying authenticity, and navigating clunky checkouts. Sole Hub aims to centralize the experience with fair raffles, a transparent release calendar, UPI-first payments, personalization, and strong anti-bot defenses.",
            "goal": [
                "Fair Access: Design verifiable, bot-resistant raffles and drop flows.",
                "Personalization: Tailor alerts and curation by size, preferences, and history.",
                "Seamless Checkout: Lead with UPI, support cards/EMI, and enable fast refunds.",
                "Community & Exclusivity: Build perks—early access, members-only content, and creator-led drops.",
                "Scalability: Optimize for patchy networks and low-end devices across Tier-1 to Tier-3 cities."
            ],
            "timeLine": {
                "researchPhase": "Month 1",
                "wireframePhase": "Month 2",
                "designHifi": "Month 3-4",
                "finalDesign": "Month 4",
                "prototype": "Month 4-5",
                "development": "Month 5-6",
                "testing": "Month 6",
                "launch": "Month 7"
            },
            "research": {
                "goal": "Understand motivations, trust barriers, and behaviors in limited drop purchases across cities and price bands.",
                "behaviorTrends": {
                    "trend1": "UPI dominance for payments and refunds.",
                    "trend2": "Calendar-first planning for drops and restocks.",
                    "trend3": "Creator-led drops drive discovery and trust.",
                    "trend4": "Resale verification is a must-have even for retail drops."
                },
                "painPoints": {
                    "point1": "Opaque raffle outcomes; suspicion of bots and backdoors.",
                    "point2": "Fragmented information across multiple channels.",
                    "point3": "Slow refunds and limited payment flexibility."
                },
                "keyfinding": {
                    "finding1": "Users want provably fair raffles with visible odds and draw proofs.",
                    "finding2": "A single, filterable release calendar reduces FOMO.",
                    "finding3": "Size-based personalization doubles alert relevance.",
                    "finding4": "UPI + express refunds increases repeat purchase intent.",
                    "finding5": "Community status and badges increase engagement."
                },
